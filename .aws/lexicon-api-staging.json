{
    "containerDefinitions": [
        {
            "name": "lexicon-api",
            "image": "public.ecr.aws/z1s6g0e3/lexicon-api:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "staging-api",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "LEXICON_DB_USER",
                    "value": "api-staging"
                },
                {
                    "name": "LEXICON_DB_HOST",
                    "value": "osfarm-8437.7tc.aws-eu-central-1.cockroachlabs.cloud"
                },
                {
                    "name": "LEXICON_DB_PORT",
                    "value": "26257"
                },
                {
                    "name": "LEXICON_DB_DATABASE",
                    "value": "lexicon"
                },
                {
                    "name": "API_PORT",
                    "value": "3000"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "LEXICON_DB_PWD",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-3:313244307761:secret:osfarm/lexicon/staging-6absQ3:LEXICON_DB_PWD::"
                }
            ],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/osfarm-lexicon-api-staging",
                    "awslogs-region": "eu-west-3",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "family": "lexicon-api",
    "executionRoleArn": "arn:aws:iam::313244307761:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "targetGroupArn": "arn:aws:elasticloadbalancing:eu-west-3:313244307761:targetgroup/lexicon-api-staging-tg/505eecd52fb44b94",
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}